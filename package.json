{
  "name": "questit",
  "private": true,
  "version": "1.0.0",
  "description": "Cloudflare-first platform for generating micro-tools from natural language",
  "type": "module",
  "main": "src/index.js",
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "micro-tools"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "workers/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "pnpm --filter web dev",
    "dev:mobile": "pnpm --filter mobile dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter web build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "turbo": "turbo",
    "generate:templates": "node scripts/generate-templates-from-csv.js",
    "import:templates": "node scripts/import-templates-to-supabase.js",
    "import:legacy-templates": "node scripts/backfill-legacy-templates.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "turbo": "^2.3.3",
    "typescript": "^5.9.3"
  }
}
